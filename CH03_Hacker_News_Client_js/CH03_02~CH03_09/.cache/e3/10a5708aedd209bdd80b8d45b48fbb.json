n\nfunction getData(url) {\n  ajax.open(\"GET\", url, false);\n  ajax.send();\n\n  return JSON.parse(ajax.response);\n}\n\nfunction newsFeed() {\n  const newsFeed = getData(NEWS_URL);\n  store.limit = Math.ceil(newsFeed.length / store.offset);\n  const newsList = [];\n  let template = `\n    <div>\n      <h1>Hacker News</h1>\n      <ul>\n        {{__news_feed__}}\n      </ul>\n      <div>\n        <a href=\"#/page/{{__prev_page__}}\">이전 페이지</a>\n        <a href=\"#/page/{{__next_page__}}\">다음 페이지</a>\n      </div>\n    </div>\n  `;\n\n  for (\n    let i = (store.currentPage - 1) * store.offset;\n    i < store.currentPage * store.offset;\n    i++\n  ) {\n    newsList.push(`\n      <li>\n        <a href=\"#/show/${newsFeed[i].id}\">\n          ${newsFeed[i].title} (${newsFeed[i].comments_count})\n        </a>\n      </li>\n    `);\n  }\n\n  template = template.replace(\"{{__news_feed__}}\", newsList.join(\"\"));\n  template = template.replace(\n    \"{{__prev_page__}}\",\n    store.currentPage > 1 ? store.currentPage - 1 : 1\n  );\n  template = template.replace(\n    \"{{__next_page__}}\",\n    store.currentPage < store.limit ? store.currentPage + 1 : store.limit\n  );\n\n  container.innerHTML = template;\n}\n\nfunction newsDetail() {\n  const id = location.hash.substring(7);\n\n  const newsContent = getData(CONTENT_URL.replace(\"@id\", id));\n\n  container.innerHTML = `\n    <h1>${newsContent.title}</h1>\n    <div>\n      <a href=\"#/page/${store.currentPage}\">목록으로</a>\n    </div>\n  `;\n}\n\nfunction router() {\n  const routePath = location.hash; // location.hash 에 '#'만 있을 경우, 빈 값('')을 반환함.\n\n  console.log(routePath);\n\n  if (routePath === \"\") {\n    newsFeed();\n  } else if (routePath.indexOf(\"#/page/\") >= 0) {\n    store.currentPage = Number(routePath.substring(7));\n    newsFeed();\n  } else {\n    newsDetail();\n  }\n}\n\nwindow.addEventListener(\"hashchange\", router);\n\nrouter();\n"},"lineCount":null}},"error":null,"hash":"2658e0a0c7abaf61202bbf9e6e1734de","cacheData":{"env":{}}}